            <TextField fx:id="TF_nom" prefHeight="12.0" prefWidth="187.0" />
            <TextField fx:id="TFPrenom" prefHeight="12.0" prefWidth="187.0" />
            <TextField fx:id="TfUsername" prefHeight="12.0" prefWidth="187.0" />
            <TextField fx:id="TF_email" />
