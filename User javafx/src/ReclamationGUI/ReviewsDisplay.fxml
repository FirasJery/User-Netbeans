<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>

<AnchorPane id="AnchorPane" prefHeight="620.0" prefWidth="1200.0" stylesheets="@../style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ReclamationGUI.ReviewsDisplayController">
   <children>
      <StackPane prefHeight="620.0" prefWidth="1200.0">
         <children>
            <Pane fx:id="PaneEntreprise" prefHeight="620.0" prefWidth="1200.0">
               <children>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="1200.0" spacing="50.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#VosRevuesAction" text="Vos Revues" />
                        <Button mnemonicParsing="false" onAction="#RevuesAction" text="Revues" />
                     </children>
                  </HBox>
                  <HBox layoutY="100.0" prefHeight="520.0" prefWidth="1200.0">
                     <children>
                        <ScrollPane fx:id="ScrollPaneEntrep" prefHeight="520.0" prefWidth="1199.0">
                          <content>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
                          </content>
                        </ScrollPane>
                     </children>
                  </HBox>
               </children>
            </Pane>
            <Pane fx:id="PaneFreelancer" prefHeight="200.0" prefWidth="200.0" visible="false">
               <children>
                  <ScrollPane fx:id="ScrollPaneFree" layoutY="-1.0" prefHeight="620.0" prefWidth="1199.0">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
                    </content>
                  </ScrollPane>
               </children>
            </Pane>
         </children>
      </StackPane>
   </children>
</AnchorPane>
