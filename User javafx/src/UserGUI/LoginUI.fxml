            </Label>
            <CheckBox fx:id="checkboxResteConnecte" layoutX="310.0" layoutY="173.0" mnemonicParsing="false" text="Resté connecté ?" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </CheckBox>
         </children>
      </Pane>
      <ImageView fitHeight="113.0" fitWidth="284.0" layoutX="43.0" layoutY="23.0" pickOnBounds="true" preserveRatio="true">
         <image>
