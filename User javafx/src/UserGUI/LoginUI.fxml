<AnchorPane id="AnchorPane" prefHeight="800.0" prefWidth="1200.0" stylesheets="@../style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UserGUI.LoginUIController">
   <children>
      <ImageView fitHeight="800.0" fitWidth="1200.0" pickOnBounds="true">
         <image>
            <Image url="@../resources/loginbg.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <Pane layoutX="576.0" layoutY="143.0" prefHeight="508.0" prefWidth="600.0">
         <children>
            <Label layoutX="137.0" layoutY="57.0" text="Email" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="30.0" />
               </font>
            </Label>
            <Label layoutX="84.0" layoutY="117.0" text="Mot de passe" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="30.0" />
               </font>
            </Label>
            <Button focusTraversable="false" layoutX="414.0" layoutY="212.0" mnemonicParsing="false" onAction="#On_annuler_clicked" text="Annuler" textFill="#1a0707">
