      <Button fx:id="BtnPrendrePhoto" layoutX="626.0" layoutY="352.0" mnemonicParsing="false" onAction="#OnPrendreClicked" prefHeight="31.0" prefWidth="151.0" text="Prendre photo" visible="false" />
