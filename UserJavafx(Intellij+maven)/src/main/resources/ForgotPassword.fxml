<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="700.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.ForgotPasswordController">
   <children>
      <VBox layoutY="-1.0" prefHeight="708.0" prefWidth="1101.0" visible="false" />
      <Pane fx:id="paneEmail" layoutX="551.0" layoutY="96.0" prefHeight="368.0" prefWidth="519.0">
         <children>
            <Label layoutX="129.0" layoutY="135.0" text="Email" />
            <Button fx:id="BtnAnnuler" layoutX="309.0" layoutY="191.0" mnemonicParsing="false" onAction="#On_annuler_clicked" prefHeight="31.0" prefWidth="88.0" text="Annuler" />
            <Button fx:id="BtnEnvoyerMail" layoutX="173.0" layoutY="191.0" mnemonicParsing="false" onAction="#OnEnvoyerMailClicked" prefHeight="31.0" prefWidth="125.0" text="Envoyer E-mail" />
            <TextField fx:id="TF_Email_reset" layoutX="188.0" layoutY="130.0" promptText="example@domain.com" />
         </children>
      </Pane>
      <Pane fx:id="paneCode" layoutX="486.0" layoutY="180.0" prefHeight="261.0" prefWidth="527.0" visible="false">
         <children>
            <TextField fx:id="tfCode" layoutX="155.0" layoutY="57.0" prefHeight="63.0" prefWidth="217.0" promptText="Votre code ici" />
            <Button fx:id="submit" layoutX="210.0" layoutY="153.0" mnemonicParsing="false" onAction="#OnsubmitClicked" prefHeight="48.0" prefWidth="108.0" text="Submit" />
         </children>
      </Pane>
      <Pane fx:id="panePassword" layoutX="497.0" layoutY="230.0" prefHeight="200.0" prefWidth="455.0" visible="false">
         <children>
            <Button fx:id="BtnModifier" layoutX="150.0" layoutY="113.0" mnemonicParsing="false" onAction="#OnModifierClicked" prefHeight="73.0" prefWidth="178.0" text="Modifer " />
            <PasswordField fx:id="tfPassword" layoutX="146.0" layoutY="23.0" promptText="Password" />
            <PasswordField fx:id="tfComfirm" layoutX="146.0" layoutY="69.0" promptText="Confirm" />
         </children>
      </Pane>
   </children>
</AnchorPane>
