                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button fx:id="Button_se_connecter" focusTraversable="false" layoutX="271.0" layoutY="212.0" mnemonicParsing="false" onAction="#ON_seconnecter_clicked" text="Se connecter">
